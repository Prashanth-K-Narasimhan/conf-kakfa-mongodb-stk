CREATE STREAM telemetry_stream (
  did VARCHAR,
  timestamp BIGINT,
  name VARCHAR,
  int_value INTEGER,
  float_value DOUBLE,
  string_value VARCHAR,
  eventTime VARCHAR,
  receivedTime VARCHAR,
  ingest_rowtime BIGINT
) WITH (
  KAFKA_TOPIC='telemetry',
  VALUE_FORMAT='JSON',
  TIMESTAMP='timestamp'
);
