                                                       "queryString":  "CREATE TABLE VEHICLE_LATEST_STATE WITH (KAFKA_TOPIC=\u0027vehicle_latest_state\u0027, PARTITIONS=1, REPLICAS=1, VALUE_FORMAT=\u0027JSON\u0027) AS SELECT\n  TELEMETRY_NORMALIZED.DID DID,\n  LATEST_BY_OFFSET(TELEMETRY_NORMALIZED.EVENTTIME) EVENTTIME,\n  LATEST_BY_OFFSET((CASE WHEN (TELEMETRY_NORMALIZED.SENSOR_NAME = \u0027odometer\u0027) THEN TELEMETRY_NORMALIZED.FLOAT_VALUE ELSE null END)) ODO,\n  LATEST_BY_OFFSET((CASE WHEN (TELEMETRY_NORMALIZED.SENSOR_NAME = \u0027soc\u0027) THEN CAST(TELEMETRY_NORMALIZED.INT_VALUE AS INTEGER) ELSE null END)) SOC,\n  LATEST_BY_OFFSET((CASE WHEN (TELEMETRY_NORMALIZED.SENSOR_NAME = \u0027speed\u0027) THEN CAST(TELEMETRY_NORMALIZED.INT_VALUE AS INTEGER) ELSE null END)) SPEED,\n  LATEST_BY_OFFSET((CASE WHEN (TELEMETRY_NORMALIZED.SENSOR_NAME = \u0027ignition_status\u0027) THEN CAST(TELEMETRY_NORMALIZED.INT_VALUE AS INTEGER) ELSE null END)) IGNITION,\n  LATEST_BY_OFFSET(TIMESTAMPTOSTRING(TELEMETRY_NORMALIZED.ROWTIME, \u0027yyyy-MM-dd\u0027\u0027T\u0027\u0027HH:mm:ss.SSSX\u0027, \u0027UTC\u0027)) INGESTION_TIME\nFROM TELEMETRY_NORMALIZED TELEMETRY_NORMALIZED\nGROUP BY TELEMETRY_NORMALIZED.DID\nEMIT CHANGES;",
